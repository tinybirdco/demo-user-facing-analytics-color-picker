TOKEN "read_endpoints" READ

NODE get_games
SQL >

    SELECT
      username,
      game_id,
      sum(duration) AS game_duration,
      count() AS clicks
    FROM game_events
    WHERE correct = 1
    GROUP BY username, game_id



NODE endpoint
SQL >

    SELECT
      username AS name,
      game_duration AS value
    FROM get_games
    WHERE clicks = 25
    ORDER BY game_duration
    LIMIT 1 BY username
    LIMIT 10


